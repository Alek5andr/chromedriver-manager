{
  "compilerOptions": {
    "types": [
      "node"
    ],
    "target": "es2019",
    "strictPropertyInitialization": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "moduleResolution": "Node",
    "esModuleInterop": true
  }
}
